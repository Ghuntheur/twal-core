{
  "name": "twal-core",
  "version": "0.1.0",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "node-sass": "^4.11.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-scripts": "2.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/gitlab": "^3.1.2",
    "@semantic-release/npm": "^5.1.4",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "cross-env": "^5.2.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "^1.3.1",
    "prettier": "1.16.4",
    "pretty-quick": "^1.10.0",
    "react-app-rewire-aliases": "^0.2.0",
    "react-app-rewired": "^2.1.0"
  },
  "scripts": {
    "dev": "cross-env PORT=8080 react-app-rewired start",
    "prod": "react-app-rewired build",
    "test:watch": "react-app-rewired test",
    "test:ci": "cross-env CI=true react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
